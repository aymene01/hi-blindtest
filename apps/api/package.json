  "bugs": {
    "url": "https://github.com/aymene01/school-api/issues"
  },
  "homepage": "https://github.com/aymene01/school-api#readme",
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "esbuild": "^0.17.4",
    "esbuild-register": "^3.4.2",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "prisma": "^4.9.0",
    "typescript": "^4.9.4",
    "toolbox": "*"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3"
  }
}
